package com.Rice.entity;

import java.io.Serializable;
import java.util.Date;

public class Recharge implements Serializable{
	private int rId;
	private Date rTime;
	private String rOrderNumber;
	private Double rMoney;
	private Member mId;
	public int getrId() {
		return rId;
	}
	public Date getrTime() {
		return rTime;
	}
	public String getrOrderNumber() {
		return rOrderNumber;
	}
	public void setrId(int rId) {
		this.rId = rId;
	}
	public void setrTime(Date rTime) {
		this.rTime = rTime;
	}
	public void setrOrderNumber(String rOrderNumber) {
		this.rOrderNumber = rOrderNumber;
	}
	
	public Double getrMoney() {
		return rMoney;
	}
	public void setrMoney(Double rMoney) {
		this.rMoney = rMoney;
	}
	public Member getmId() {
		return mId;
	}
	public void setmId(Member mId) {
		this.mId = mId;
	}
	@Override
	public String toString() {
		return "Recharge [rId=" + rId + ", rTime=" + rTime + ", rOrderNumber=" + rOrderNumber + ", rMoney=" + rMoney
				+ ", mId=" + mId + "]";
	}
	
}
